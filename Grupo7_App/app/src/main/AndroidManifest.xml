<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="grupo7.com.appg7">


    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_sin_appg7"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_sin_appg7"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".PreferenciasActivity"></activity>


        <activity
            android:name=".AcercaDeActivity"
            android:label="Acerca de ..."
            android:theme="@android:style/Theme.Dialog" />


        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PerfilActivity"></activity>
        <activity android:name=".SonoffActivity"></activity>
    </application>



    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>






</manifest>